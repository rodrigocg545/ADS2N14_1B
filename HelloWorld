import java.util.*; 
public class HelloWorld
{   public static void main(String [] args){
    String username; 
    
    Scanner reader = new Scanner(System.in);
    
    System.out.println("Digite seu username!"); 
    username = reader.next();
    System.out.println("Hello "+ username+ "!"); 
}
}
